<% include ../partials/header %>
<br>
<div class="jumbotron bg-link "> 
<h1 class="display-3 text-center">USERS REPORT</h1>
<h1 class="text-center">Report:Top 25 Customers by Sales</h1>
<table class="table">
    <thead>
        <tr>
            <th>Customers ID</th>
            <th>Name</th>
            <th>Email</th>
            <th>Total Sales</th>
        </tr>
    </thead>
    <tbody>
    <% user.forEach(function(user) { %>
        <tr>
            <td scope="row"><%= user.user_id %></td>
            <td><%= user.name %></td>
            <td><%= user.email %></td>
            <td>$<%= user.price %></td>
        </tr>
        <% }); %>
    </tbody>
</table>
 
<% include ../partials/footer %>
</div>