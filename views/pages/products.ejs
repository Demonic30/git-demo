<% include ../partials/header %>

<table class="table">
    <thead>
        <tr>
            <th>ID</th>
            <th>Title</th>
            <th>Price</th>
            <th>Edit</th>
            <th>Delete</th>
            <th> <a class="btn btn-success" href="/insert" role="button">Insert</a></th>
        </tr>
    </thead>
    <tbody>
    <% products.forEach(function(product) { %>
        <tr>
            <td scope="row"><%= product.id %></td>
            <td><%= product.title %></td>
            <td><%= product.price %></td>
            <td>
            <a class="btn btn-warning" href="/product/<%= product.id%>" role="button">Edit</a>
            </td>
            <td>
            <a class="btn btn-danger" href="" role="button">Delete</a>
            </td>
        </tr>
        <% }); %>
    </tbody>
</table>



<% include ../partials/footer %>